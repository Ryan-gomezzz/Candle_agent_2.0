{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/enquire",
      "dest": "api/index.js"
    },
    {
      "src": "/webhook",
      "dest": "api/index.js"
    },
    {
      "src": "/leads",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}

